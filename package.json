{
  "name": "web_app",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "concurrently \"npm run app:dev\" \"npm run css:watch\"",
    "css:build": "node-sass-chokidar --include-path ./scss --include-path ./node_modules scss/main.scss public/css/main.css",
    "css:watch": "nodemon -e scss -x \"npm run css:build\"",
    "app:dev": "node scripts/start.js",
    "app:build": "node scripts/build.js",
    "app:test": "node scripts/test.js --env=jsdom",
    "heroku-postbuild": "npm run app:build",
    "heroku": "./node_modules/.bin/http-server ./build -p $PORT -f ./build/index.html"
  },
  "proxy": "http://localhost:7001",
  "dependencies": {
    "@babel/standalone": "^7.0.0-beta.32",
    "add": "^2.0.6",
    "annyang": "^2.6.0",
    "apollo-client-preset": "^1.0.1",
    "apollo-link-ws": "^1.0.0",
    "autobind-decorator": "^2.1.0",
    "autoprefixer": "7.1.6",
    "babel-core": "6.26.0",
    "babel-eslint": "7.2.3",
    "babel-jest": "20.0.3",
    "babel-loader": "7.1.2",
    "babel-plugin-transform-decorators": "^6.24.1",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-preset-react-app": "^3.1.0",
    "babel-runtime": "6.26.0",
    "bigi": "^1.4.2",
    "bitcoinjs-lib": "^3.3.1",
    "blockchain.info": "^2.10.1",
    "bulma": "^0.6.0",
    "case-sensitive-paths-webpack-plugin": "2.1.1",
    "chalk": "1.1.3",
    "circular-json": "^0.5.1",
    "copy-to-clipboard": "^3.0.8",
    "create-react-class": "^15.6.2",
    "crypto-js": "^3.1.9-1",
    "css-loader": "0.28.7",
    "dotenv": "4.0.0",
    "eslint": "4.10.0",
    "eslint-config-react-app": "^2.0.1",
    "eslint-loader": "1.9.0",
    "eslint-plugin-flowtype": "2.39.1",
    "eslint-plugin-import": "2.8.0",
    "eslint-plugin-jsx-a11y": "5.1.1",
    "eslint-plugin-react": "7.4.0",
    "eventemitter3": "^3.0.0",
    "extract-text-webpack-plugin": "3.0.2",
    "file-loader": "1.1.5",
    "fs-extra": "3.0.1",
    "getstream": "^3.9.0",
    "github-api": "3.0.0",
    "graphql": "^0.11.7",
    "graphql-tag": "^2.5.0",
    "handlebars": "^4.0.11",
    "hashcash": "^0.5.1",
    "html-webpack-plugin": "2.29.0",
    "http-server-fallback": "0.8.5",
    "ipfs": "0.28.0",
    "jest": "20.0.4",
    "jquery": "^3.2.1",
    "js-schema": "^1.0.1",
    "js-sha256": "0.9.0",
    "jszip": "3.1.5",
    "jszip-utils": "0.0.2",
    "localforage": "^1.5.3",
    "lodash": "^4.17.4",
    "memfs": "^2.6.0",
    "mkdirp": "^0.5.1",
    "mkdirp-promise": "^5.0.1",
    "moment": "^2.19.2",
    "multihashes": "0.4.13",
    "node-rsa": "^0.4.2",
    "node-sass-chokidar": "^0.0.3",
    "object-assign": "4.1.1",
    "parse-github-url": "1.0.2",
