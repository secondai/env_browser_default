    "promise": "8.0.1",
    "queue": "^4.4.2",
    "radium": "^0.19.6",
    "raf": "3.4.0",
    "react": "^16.0.0",
    "react-ace": "^5.5.0",
    "react-apollo": "^2.0.0",
    "react-autosize-textarea": "^0.4.9",
    "react-contextmenu": "^2.9.0",
    "react-dev-utils": "^4.2.1",
    "react-dom": "^16.0.0",
    "react-emojione": "^5.0.0",
    "react-moment": "^0.6.8",
    "react-resize-detector": "^1.1.0",
    "react-router-dom": "^4.2.2",
    "react-select": "^1.1.0",
    "react-table": "^6.7.4",
    "react-tippy": "^1.2.2",
    "react-tooltip": "^3.4.0",
    "recharts": "^1.0.0-beta.1",
